{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "1.3.4",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "axios": "0.21.1",
    "lodash": "4.17.20",
    "node-sass": "4.14.1",
    "react": "16.13.1",
    "react-color": "^2.19.3",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.3",
    "react-select": "3.1.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "2.0.1",
    "three": "0.120.1"
  },
  "devDependencies": {
    "prettier": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*_pb.js"
        ],
        "rules": {
          "no-undef": "off",
          "no-new-func": "off",
          "no-unused-vars": "off",
          "new-parens": "off",
          "no-redeclare": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
